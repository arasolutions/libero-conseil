                                            <img
                                                    src="https://www.kiwicloudcms.com/content/picture/id/{{ reference.logo }}/ql/1140x705"
                                                    alt="" class="reference">
                                        </a>
                                        <div class="overlay-to-top">
                                            <a href="{{ reference.link }}" class="small" target="_blank">{{ reference.nom|upper }}</a>
                                        </div>
                                    {% else %}
                                        <img
                                                src="https://www.kiwicloudcms.com/content/picture/id/{{ reference.logo }}/ql/1140x705"
                                                alt="" class="reference">
                                        <div class="overlay-to-top">
                                            <span class="small">{{ reference.nom|upper }}</span>
                                        </div>
                                    {% endif %}
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>
